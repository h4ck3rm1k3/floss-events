    <!-- http://www.ittags.com/ontology/ittags.owl#Blog -->

    <owl:Class rdf:about="#Blog">
        <rdfs:subClassOf rdf:resource="#SocialMedia"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasTitle"/>
                <owl:allValuesFrom rdf:resource="#Blog"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasStory"/>
                <owl:allValuesFrom rdf:resource="#Blog"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The main container for an ITTAGS Technology Blog post. A Technology Blog is authroed by a Technology Company or a Technology Person.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ittags.com/ontology/ittags.owl#BlogSocialNetworkWebsite -->

    <owl:Class rdf:about="#BlogSocialNetworkWebsite">
        <rdfs:label>Blog Website</rdfs:label>
        <rdfs:subClassOf rdf:resource="#SocialNetworkWebsite"/>
    </owl:Class>
    


    <!-- http://www.ittags.com/ontology/ittags.owl#BoardMember -->

    <owl:Class rdf:about="#BoardMember">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#TechPerson"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#isBoardMemberOf"/>
                        <owl:someValuesFrom rdf:resource="#TechCompany"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.ittags.com/ontology/ittags.owl#BusinessWebsite -->

    <owl:Class rdf:about="#BusinessWebsite">
        <rdfs:label>Business Network Website</rdfs:label>
