    </owl:Class>
    


    <!-- http://www.ittags.com/ontology/ittags.owl#City -->

    <owl:Class rdf:about="#City">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Location"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#isCityOf"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.ittags.com/ontology/ittags.owl#Client -->

    <owl:Class rdf:about="#Client">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isClientOf"/>
                <owl:someValuesFrom rdf:resource="#TechCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>This class describes any Company or Person who is not a Technology Company or Technology Person but who a Technology Client relationship with .  It also describes a Client relationships with a Technical Recruiter established exclusively for the purpose of filling a Technology Job within a Company that is not a Technology Company.</rdfs:comment>
    </owl:Class>
    


