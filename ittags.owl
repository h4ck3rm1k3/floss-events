    <owl:Class rdf:about="#CompanyBlog">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Blog"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasAuthor"/>
                        <owl:someValuesFrom rdf:resource="#TechCompany"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="#PersonalBlog"/>
